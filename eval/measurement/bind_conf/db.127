;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	localhost.
1.0.0	IN	PTR	localhost1.
2.0.0	IN	PTR	localhost2.
3.0.0	IN	PTR	localhost3.
4.0.0	IN	PTR	localhost4.
