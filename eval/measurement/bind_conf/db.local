;
; BIND data file for local loopback interface
;
$TTL	3600
@	IN	SOA	localhost. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  3600 )	; Negative Cache TTL
;
@	IN	NS	localhost.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

host1	IN	A	127.0.0.1
host2	IN	A	127.0.0.2
host3	IN	A	127.0.0.3
host4	IN	A	127.0.0.4
host5	IN	A	127.0.0.5
host6	IN	A	127.0.0.6
host7	IN	A	127.0.0.7
host8	IN	A	127.0.0.8
host9	IN	A	127.0.0.9
host10	IN	A	127.0.0.10
